<?xml version="1.0"?>
<launch>
  <arg name="device_ip" default="192.168.1.201"/>
  <arg name="return_mode_40P" default="Dual"/> <!-- Strongest / Last / Dual -->
  <arg name="return_mode_QT" default="Dual"/> <!-- First / Last / Dual -->
  <group>
  <push-ros-namespace namespace="pandar40P_0"/>
  <arg name="lidar_port_40P_0"  default="10000"/>
  <arg name="gps_port_40P_0"  default="10100"/>
  <arg name="frame_id_40P_0" default="40P_0"/>
  <include file="$(find-pkg-share pandar_pointcloud)/launch/pandar40P_awsim.launch.xml">
    <arg name="lidar_port" value="$(var lidar_port_40P_0)"/>
    <arg name="gps_port"   value="$(var gps_port_40P_0)"/>
    <arg name="frame_id"  value="$(var frame_id_40P_0)"/>
    <arg name="device_ip" value="$(var device_ip)"/>
    <arg name="return_mode"  value="$(var return_mode_40P)"/>
  </include>
  <node pkg="tf2_ros" exec="static_transform_publisher" name="tool_broadcaster" args="0 -10 -5 0.1305262 0 0 0.9914449 map 40P_0" />
  </group>

